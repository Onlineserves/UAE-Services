<script> 
    $(document).ready(function () {
        update_recentSearch();
        const searchInput = document.querySelector(".search-container-mobile");
        const input = searchInput.querySelector("input");
        const resultBox = $(".search-result-mobile");


        $("#search-form-control-mobile").submit(function (e) {
            event.preventDefault();
            let userData = $(".search-container-mobile input").val();
            document.querySelectorAll('#loader-search-mobile').forEach(block => block.style.display = 'none');
            onSearchPresEnter(userData);
        });
         
        $(document).on('keyup', '.search-container-mobile input', function (e) {
            let userData = e.target.value 
            $('.search-close-btn').css('display', 'block'); 
            if (userData && userData.length >= 3) { 
                document.querySelectorAll('#loader-search-mobile').forEach(block => block.style.display = 'block');
                $.ajax({
                    url: "/api/Search/GetPages",
                    method: "GET",
                    data: {
                        query: userData,
                        maxRows: 5,
                        indexCatalogue: getSearchIndex()
                    },
                    dataType: "json",
                    success: function (data) {
                        document.querySelectorAll('#loader-search-mobile').forEach(block => block.style.display = 'none'); 
                        resultBox.empty();
                        $.each(data, function (i, obj) {
                            const suggestElement = $('<li class="suggestion"></li>')
                            suggestElement.text(obj.title);
                            suggestElement.on('click', function () {
                                add_item_search_item_in_cookie(obj.title, obj.link);
                                window.location.href = obj.link;
                            });
                            resultBox.append(suggestElement);
                        });
                        searchInput.classList.add("active");
                    },
                    error: function (xhr, status, error) {
                        document.querySelectorAll('#loader-search-mobile').forEach(block => block.style.display = 'none'); 
                    }
                });
            } else if (userData.length <= 0) {
                reset_inputBar();
                document.querySelectorAll('#loader-search-mobile').forEach(block => block.style.display = 'none');
            }
        });

        function reset_inputBar() {
            input.value = "";
            resultBox.empty();
            $('.search-close-btn').css('display', 'none');
            document.querySelectorAll('#loader-search-mobile').forEach(block => block.style.display = 'none');
            input.focus();
        } 

        $(document).on("click", ".search-form-icon, .search-close-btn", function (e) {
            reset_inputBar();
        });
    });
</script>
