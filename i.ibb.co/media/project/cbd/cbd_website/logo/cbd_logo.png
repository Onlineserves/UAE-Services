    <script>

        function validatePhoneNumberInput(input) {
            var inputString = input.value;
            var result = '';

            for (var i = 0; i < inputString.length; i++) {
                if (inputString[i] === '5') {
                    result += inputString[i];
                    i++;
                    while (i < inputString.length && !isNaN(Number(inputString[i]))) {
                        result += inputString[i];
                        i++;
                    }
                } else {
                    break;
                }
            }
            return result;
        }



        var inputCustomPhone = document.getElementById('mobile-phone-contact');
        inputCustomPhone.addEventListener('input', function () {
            inputCustomPhone.value = validatePhoneNumberInput(inputCustomPhone);
