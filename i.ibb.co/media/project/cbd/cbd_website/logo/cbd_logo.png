        $("#main-mobile-form").submit(function (event) {
            $('#error-message-phone').hide();
            event.preventDefault();
            var phoneNumber = document.getElementById("mobile-phone-contact").value;
            if (!validateNumberCheck(phoneNumber)) {
                document.getElementById('error-message-phone').style.display = 'block';
                $('#error-message-phone').text('Please enter valid mobile number.');
                return false;
            }


            document.getElementById('mobile-form-sms').style.display = 'none';
            document.getElementById('loader-main-contact-download').style.display = 'block';

            $.ajax({
                type: "POST",
                url: "/MobileAppPromo/submitresponse",
                data: $("#main-mobile-form").serialize(),
                complete: function (response) {
                    var data = JSON.parse(response.responseText); 
                    if (data.success) {
                        document.getElementById('loader-main-contact-download').style.display = 'none';
                        document.getElementById('success-message-mobile-apply').style.display = 'block';
                    } else {
                        document.getElementById('error-message-phone').style.display = 'block';
                        $('#error-message-phone').text(data.message);
                        document.getElementById('mobile-form-sms').style.display = 'block';
                        document.getElementById('success-message-mobile-apply').style.display = 'none';
                        document.getElementById('loader-main-contact-download').style.display = 'none';
                    }
                },
                success: function (result) {

                },
                error: function (result) {
                    document.getElementById('mobile-form-sms').style.display = 'block';
                    document.getElementById('success-message-mobile-apply').style.display = 'none';
                    document.getElementById('loader-main-contact-download').style.display = 'none';
                }
            });
        });

    </script>









